
# ===== Operaciones del mock =====
extend type Query {
    # Retorna las camillas simuladas en memoria
    camillasDisponiblesMock: [CamillaDocument]

    # Busca una camilla simulada por su ID
    camillaPorIdMock(id: ID!): CamillaDocument
}

extend type Mutation {
    # Libera una camilla del mock
    liberarCamillaMock(idCamilla: ID!, fechaFin: String!): CamillaDocument
}

